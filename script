coroutine.wrap(FMUJV_fake_script)()
local function GYSH_fake_script() -- wallbangtoggle.LocalScript 
	local script = Instance.new('LocalScript', wallbangtoggle)

	-- LocalScript INSIDE THE TOOL

	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")

	local player = Players.LocalPlayer
	local camera = workspace.CurrentCamera
	local mouse = player:GetMouse()

	local OFFSET_BEHIND = -3
	local RETURN_DELAY = 0.1

	local function getClosestPlayerToMouse()
		local closest, closestDist = nil, math.huge

		for _, plr in ipairs(Players:GetPlayers()) do
			if plr ~= player
				and plr.Character
				and plr.Character:FindFirstChild("HumanoidRootPart") then

				local pos, onScreen = camera:WorldToViewportPoint(
					plr.Character.HumanoidRootPart.Position
				)

				if onScreen then
					local dist = (Vector2.new(pos.X, pos.Y)
						- Vector2.new(mouse.X, mouse.Y)).Magnitude

					if dist < closestDist then
						closestDist = dist
						closest = plr
					end
				end
			end
		end

		return closest
	end

	script.Parent.Activated:Connect(function()
		-- Toggle check
		local toggle = player.PlayerGui:FindFirstChild("YourGuiNameHere", true)
		if toggle then
			local button = toggle:FindFirstChild("YourButtonNameHere", true)
			if button and string.lower(button.Text) ~= "on" then
				return
			end
		end

		local char = player.Character
		if not char then return end

		local hrp = char:FindFirstChild("HumanoidRootPart")
		if not hrp then return end

		local target = getClosestPlayerToMouse()
		if not target or not target.Character then return end

		local targetHRP = target.Character:FindFirstChild("HumanoidRootPart")
		if not targetHRP then return end

		local original = hrp.CFrame

		-- TELEPORT
		hrp.CFrame = targetHRP.CFrame * CFrame.new(0, 0, OFFSET_BEHIND)

		-- Ensure teleport applies BEFORE shot
		RunService.Heartbeat:Wait()

		-- Tool fires automatically (this is Activated)

		-- TELEPORT BACK
		task.delay(RETURN_DELAY, function()
			if hrp then
				hrp.CFrame = original
			end
		end)
	end)

end
coroutine.wrap(GYSH_fake_script)()
