-- Scripts:

local function DXLUR_fake_script() -- legitlock.LocalScript 
	local script = Instance.new('LocalScript', legitlock)

	local Players = game:GetService("Players")
	local player = Players.LocalPlayer

	local locktoggle = player.PlayerGui.pwrware.Frame.Frame.Frame.aimbotgui.legitlock.lockaimtoggle
	local fovshow = player.PlayerGui.pwrware.Frame.Frame.Frame.aimbotgui.legitlock.showfovtoggle
	local smoothnesstoggle = player.PlayerGui.pwrware.Frame.Frame.Frame.aimbotgui.legitlock.smoothnesstoggle

	locktoggle.Text = "off"
	fovshow.Text = "off"
	smoothnesstoggle.Text = "off"

	local clickSound = Instance.new("Sound")
	clickSound.SoundId = "rbxassetid://125027148509088"
	clickSound.Volume = 1
	clickSound.Parent = player.PlayerGui

	local soundReady = false

	clickSound.Loaded:Connect(function()
		soundReady = true
	end)

	clickSound:Play()
	clickSound:Stop()

	local function playClick()
		if not soundReady then
			clickSound.Loaded:Wait()
			soundReady = true
		end

		clickSound:Stop()
		clickSound.TimePosition = 0
		clickSound.PlaybackSpeed = math.random(92, 108) / 100
		clickSound:Play()
	end

	locktoggle.MouseButton1Click:Connect(function()
		playClick()
		locktoggle.Text = (locktoggle.Text == "off") and "on" or "off"
	end)

	fovshow.MouseButton1Click:Connect(function()
		playClick()
		fovshow.Text = (fovshow.Text == "off") and "on" or "off"
	end)

	smoothnesstoggle.MouseButton1Click:Connect(function()
		playClick()
		smoothnesstoggle.Text = (smoothnesstoggle.Text == "off") and "on" or "off"
	end)

end
coroutine.wrap(DXLUR_fake_script)()
local function CITLS_fake_script() -- sliderbar.LocalScript 
	local script = Instance.new('LocalScript', sliderbar)

	local player = game.Players.LocalPlayer
	local slider = player.PlayerGui.pwrware.Frame.Frame.Frame.aimbotgui.legitlock.sliderbar.slider
	local dragging = false
	local fill = player.PlayerGui.pwrware.Frame.Frame.Frame.aimbotgui.legitlock.sliderbar.fill
	local mouse = player:GetMouse()
	local sliderBar = script.Parent

	slider.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging = true
		end
	end)

	slider.InputEnded:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging = false
		end
	end)

	game:GetService("UserInputService").InputChanged:Connect(function(input)
		if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
			local x = math.clamp(
				(mouse.X - sliderBar.AbsolutePosition.X) / sliderBar.AbsoluteSize.X,
				0,
				1
			)

			slider.Position = UDim2.new(x, 0, 0, 0)
			fill.Size = UDim2.new(x, 0, 1, 0)
		end
	end)

end
coroutine.wrap(CITLS_fake_script)()
local function ROGFGH_fake_script() -- sliderbarfov.LocalScript 
	local script = Instance.new('LocalScript', sliderbarfov)

	local player = game.Players.LocalPlayer
	local slider = player.PlayerGui.pwrware.Frame.Frame.Frame.aimbotgui.legitlock.sliderbarfov.slider
	local dragging = false
	local fill = player.PlayerGui.pwrware.Frame.Frame.Frame.aimbotgui.legitlock.sliderbarfov.fill
	local mouse = player:GetMouse()
	local sliderBar = script.Parent

	slider.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging = true
		end
	end)

	slider.InputEnded:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging = false
		end
	end)

	game:GetService("UserInputService").InputChanged:Connect(function(input)
		if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
			local x = math.clamp(
				(mouse.X - sliderBar.AbsolutePosition.X) / sliderBar.AbsoluteSize.X,
				0,
				1
			)

			slider.Position = UDim2.new(x, 0, 0, 0)
			fill.Size = UDim2.new(x, 0, 1, 0)
		end
	end)

end
coroutine.wrap(ROGFGH_fake_script)()
local function LGVPYBR_fake_script() -- keybindtoggle.LocalScript 
	local script = Instance.new('LocalScript', keybindtoggle)

	local Players = game:GetService("Players")
	local UserInputService = game:GetService("UserInputService")

	local player = Players.LocalPlayer
	local textButton = script.Parent

	local binding = false
	local inputConnection

	local clickSound = Instance.new("Sound")
	clickSound.SoundId = "rbxassetid://125027148509088"
	clickSound.Volume = 1
	clickSound.Parent = player.PlayerGui

	local soundReady = false

	clickSound.Loaded:Connect(function()
		soundReady = true
	end)

	clickSound:Play()
	clickSound:Stop()

	local function playClick()
		if not soundReady then
			clickSound.Loaded:Wait()
			soundReady = true
		end

		clickSound:Stop()
		clickSound.TimePosition = 0
		clickSound.PlaybackSpeed = math.random(92, 108) / 100
		clickSound:Play()
	end

	local function bindKey()
		if binding then return end
		binding = true

		playClick()
		textButton.Text = "Setting"

		inputConnection = UserInputService.InputBegan:Connect(function(input)
			local keyText = ""

			if input.UserInputType == Enum.UserInputType.Keyboard then
				keyText = input.KeyCode.Name
			elseif input.UserInputType == Enum.UserInputType.MouseButton1 then
				keyText = "MouseButton1"
			elseif input.UserInputType == Enum.UserInputType.MouseButton2 then
				keyText = "MouseButton2"
			elseif input.UserInputType == Enum.UserInputType.MouseButton3 then
				keyText = "MouseButton3"
			else
				keyText = input.UserInputType.Name
			end

			textButton.Text = keyText
			playClick()

			if inputConnection then
				inputConnection:Disconnect()
				inputConnection = nil
			end

			binding = false
		end)
	end

	textButton.MouseButton1Click:Connect(bindKey)

end
