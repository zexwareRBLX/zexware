coroutine.wrap(YRPZUHH_fake_script)()
local function QIRN_fake_script() -- silentaimtoggle.LocalScript 
	local script = Instance.new('LocalScript', silentaimtoggle)

	local Players = game:GetService("Players")
	local UIS = game:GetService("UserInputService")
	local RunService = game:GetService("RunService")
	local GuiService = game:GetService("GuiService")
	local camera = workspace.CurrentCamera

	local player = Players.LocalPlayer
	local button = script.Parent

	local silentGui = player.PlayerGui.pwrware.Frame.Frame.Frame.aimbotgui.silent
	local showFOVToggle = silentGui.showfovtoggle
	local sliderBar = silentGui.sliderbar
	local slider = sliderBar.slider

	-- FOV SETTINGS
	local MIN_FOV = 50
	local MAX_FOV = 400
	local currentFOV = 200

	-- ===== FOV CIRCLE =====
	local screenGui = player.PlayerGui.pwrware

	local fovCircle = Instance.new("Frame")
	fovCircle.AnchorPoint = Vector2.new(0.5, 0.5)
	fovCircle.BackgroundTransparency = 1
	fovCircle.Visible = false
	fovCircle.Parent = screenGui

	local corner = Instance.new("UICorner")
	corner.CornerRadius = UDim.new(1, 0)
	corner.Parent = fovCircle

	local stroke = Instance.new("UIStroke")
	stroke.Thickness = 1
	stroke.Color = Color3.fromRGB(255, 255, 255)
	stroke.Parent = fovCircle

	-- ===== UPDATE FOV FROM SLIDER =====
	local function updateFOV()
		local alpha = math.clamp(slider.Position.X.Scale, 0, 1)
		currentFOV = MIN_FOV + (MAX_FOV - MIN_FOV) * alpha
		fovCircle.Size = UDim2.fromOffset(currentFOV, currentFOV)
	end

	-- ===== TARGETING =====
	local function getClosestHead()
		local mousePos = UIS:GetMouseLocation()
		local closest, closestDist = nil, math.huge

		for _, plr in ipairs(Players:GetPlayers()) do
			if plr ~= player and plr.Character then
				local head = plr.Character:FindFirstChild("Head")
				if head then
					local screenPos, onScreen = camera:WorldToViewportPoint(head.Position)
					if onScreen then
						local dist = (Vector2.new(screenPos.X, screenPos.Y) - mousePos).Magnitude
						if dist <= currentFOV / 2 and dist < closestDist then
							closestDist = dist
							closest = head
						end
					end
				end
			end
		end

		return closest
	end

	-- ===== VISUAL LOOP =====
	RunService.RenderStepped:Connect(function()
		updateFOV()

		if showFOVToggle.Text:lower() == "on" then
			local mousePos = UIS:GetMouseLocation()
			local inset = GuiService:GetGuiInset()

			fovCircle.Visible = true
			fovCircle.Position = UDim2.new(
				0,
				mousePos.X,
				0,
				mousePos.Y - inset.Y
			)
		else
			fovCircle.Visible = false
		end
	end)

	-- ===== FLICK AIM =====
	UIS.InputBegan:Connect(function(input, processed)
		if processed then return end
		if input.UserInputType ~= Enum.UserInputType.MouseButton1 then return end
		if button.Text:lower() ~= "on" then return end

		local target = getClosestHead()
		if not target then return end

		local originalCFrame = camera.CFrame

		RunService.RenderStepped:Wait()
		camera.CFrame = CFrame.new(originalCFrame.Position, target.Position)

		RunService.RenderStepped:Wait()
		camera.CFrame = originalCFrame
	end)

end
coroutine.wrap(QIRN_fake_script)()
