coroutine.wrap(PXALCIB_fake_script)()
local function QQRQUS_fake_script() -- walkspeedtoggle.LocalScript 
	local script = Instance.new('LocalScript', walkspeedtoggle)

	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")

	local player = Players.LocalPlayer
	local button = script.Parent

	-- UI references
	local gui = player.PlayerGui.pwrware.Frame.Frame.Frame.playergui.playermods
	local sliderBar = gui["walkspeed-sliderbar"]
	local slider = sliderBar.slider

	-- SETTINGS
	local DEFAULT_SPEED = 16
	local MIN_SPEED = 16
	local MAX_SPEED = 160

	local function getHumanoid()
		local char = player.Character
		if not char then return nil end
		return char:FindFirstChildOfClass("Humanoid")
	end

	local function setDefaultSpeed()
		local humanoid = getHumanoid()
		if humanoid then
			humanoid.WalkSpeed = DEFAULT_SPEED
		end
	end

	local function applySpeedFromSlider()
		local humanoid = getHumanoid()
		if not humanoid then return end

		local x = slider.Position.X.Scale
		local speed = MIN_SPEED + (MAX_SPEED - MIN_SPEED) * x
		humanoid.WalkSpeed = speed
	end

	-- When toggle is clicked
	button.MouseButton1Click:Connect(function()
		if button.Text:lower() == "on" then
			-- Enable walkspeed mod
			applySpeedFromSlider()
		else
			-- Disable walkspeed mod  force reset
			setDefaultSpeed()
		end
	end)

	-- While enabled, keep updating speed smoothly
	RunService.RenderStepped:Connect(function()
		if button.Text:lower() == "on" then
			applySpeedFromSlider()
		end
	end)

	-- On respawn
	player.CharacterAdded:Connect(function()
		task.wait(0.2)
		if button.Text:lower() == "on" then
			applySpeedFromSlider()
		else
			setDefaultSpeed()
		end
	end)

end
coroutine.wrap(QQRQUS_fake_script)()
local function LGYISG_fake_script() -- keybindtoggle_2.LocalScript 
	local script = Instance.new('LocalScript', keybindtoggle_2)

	local Players = game:GetService("Players")
	local UserInputService = game:GetService("UserInputService")
	local SoundService = game:GetService("SoundService")

	local player = Players.LocalPlayer
	local textButton = script.Parent

	local boundInputType
	local boundKey
	local isVisible = true

	-- === CLICK SOUND (FIXED) ===
	local clickSound = Instance.new("Sound")
	clickSound.SoundId = "rbxassetid://125027148509088"
	clickSound.Volume = 1
	clickSound.Parent = SoundService --  IMPORTANT FIX

	local soundReady = false
	clickSound.Loaded:Connect(function()
		soundReady = true
	end)

	-- warm-up decode
	clickSound:Play()
	clickSound:Stop()

	local function playClick()
		if not soundReady then
			clickSound.Loaded:Wait()
			soundReady = true
		end

		clickSound:Stop()
		clickSound.TimePosition = 0
		clickSound.PlaybackSpeed = math.random(92,108) / 100
		clickSound:Play()
	end

	-- === BIND KEY ===
	local function bindKey()
		playClick()
		textButton.Text = "Setting"

		local inputConnection
		inputConnection = UserInputService.InputBegan:Connect(function(input, gameProcessed)
			if gameProcessed then return end

			if input.UserInputType == Enum.UserInputType.Keyboard then
				boundInputType = Enum.UserInputType.Keyboard
				boundKey = input.KeyCode.Name
				textButton.Text = boundKey

			elseif input.UserInputType == Enum.UserInputType.MouseButton1 then
				boundInputType = Enum.UserInputType.MouseButton1
				boundKey = "MouseButton1"
				textButton.Text = boundKey

			elseif input.UserInputType == Enum.UserInputType.MouseButton2 then
				boundInputType = Enum.UserInputType.MouseButton2
				boundKey = "MouseButton2"
				textButton.Text = boundKey

			elseif input.UserInputType == Enum.UserInputType.MouseButton3 then
				boundInputType = Enum.UserInputType.MouseButton3
				boundKey = "MouseButton3"
				textButton.Text = boundKey
			end

			playClick()
			inputConnection:Disconnect()
		end)
	end

	textButton.MouseButton1Click:Connect(bindKey)

	-- === LISTEN FOR BOUND KEY ===
	UserInputService.InputBegan:Connect(function(input, gameProcessed)
		if gameProcessed then return end
		if not boundInputType then return end

		local triggered = false

		if boundInputType == Enum.UserInputType.Keyboard then
			triggered = input.UserInputType == Enum.UserInputType.Keyboard and input.KeyCode.Name == boundKey
		else
			triggered = input.UserInputType == boundInputType
		end

		if triggered then
			local pwrware = player.PlayerGui:FindFirstChild("pwrware")
			if pwrware then
				isVisible = not isVisible
				pwrware.Frame.Visible = isVisible
				playClick()
			end
		end
	end)

end
coroutine.wrap(LGYISG_fake_script)()
local function PHHINQP_fake_script() -- Frame_3.LocalScript 
	local script = Instance.new('LocalScript', Frame_3)

	local Players = game:GetService("Players")
	local player = Players.LocalPlayer

	local aimbotgui = player.PlayerGui.pwrware.Frame.Frame.Frame.aimbotgui
	local visualsgui = player.PlayerGui.pwrware.Frame.Frame.Frame.visualsgui
	local playergui = player.PlayerGui.pwrware.Frame.Frame.Frame.playergui
	local miscgui = player.PlayerGui.pwrware.Frame.Frame.Frame.miscgui

	local aimbotbutton = player.PlayerGui.pwrware.Frame.Frame.aimbot
	local visualsbutton = player.PlayerGui.pwrware.Frame.Frame.visuals
	local miscbutton = player.PlayerGui.pwrware.Frame.Frame.misc
	local playerbutton = player.PlayerGui.pwrware.Frame.Frame.player

	local logo1 = player.PlayerGui.pwrware.Frame.Frame.Frame.logo1
	local logo2 = player.PlayerGui.pwrware.Frame.Frame.Frame.logo2
	local logo3 = player.PlayerGui.pwrware.Frame.Frame.Frame.logo3
	local logo4 = player.PlayerGui.pwrware.Frame.Frame.Frame.logo4

	logo1.Visible = true
	logo2.Visible = true
	logo3.Visible = true
	logo4.Visible = true

	-- === CLICK SOUND ===
	local clickSound = Instance.new("Sound")
	clickSound.SoundId = "rbxassetid://125027148509088"
	clickSound.Volume = 1
	clickSound.Parent = player.PlayerGui

	local soundReady = false
	clickSound.Loaded:Connect(function()
		soundReady = true
	end)

	clickSound:Play()
	clickSound:Stop()

	local function playClick()
		if not soundReady then
			clickSound.Loaded:Wait()
			soundReady = true
		end

		clickSound:Stop()
		clickSound.TimePosition = 0
		clickSound.PlaybackSpeed = math.random(92, 108) / 100
		clickSound:Play()
	end

	-- === UI LOGIC ===
	local function hideall()
		aimbotgui.Visible = false
		visualsgui.Visible = false
		playergui.Visible = false
		miscgui.Visible = false
		logo1.Visible = false
		logo2.Visible = false
		logo3.Visible = false
		logo4.Visible = false
	end

	aimbotbutton.MouseButton1Click:Connect(function()
		playClick()
		hideall()
		aimbotgui.Visible = true
	end)

	visualsbutton.MouseButton1Click:Connect(function()
		playClick()
		hideall()
		visualsgui.Visible = true
	end)

	playerbutton.MouseButton1Click:Connect(function()
		playClick()
		hideall()
		playergui.Visible = true
	end)

	miscbutton.MouseButton1Click:Connect(function()
		playClick()
		hideall()
		miscgui.Visible = true
	end)

end
coroutine.wrap(PHHINQP_fake_script)()
local function YCBRB_fake_script() -- Frame.LocalScript 
	local script = Instance.new('LocalScript', Frame)

	local gui = script.Parent -- The frame to drag
	local dragging = false
	local dragInput
	local dragStart
	local startPos

	-- Optional: create a specific area inside the frame to drag
	-- If you want the whole frame draggable, just use gui itself
	local dragHandle = gui:WaitForChild("DragArea") -- e.g., a small Frame or TextLabel at the top

	-- Update position
	local function update(input)
		local delta = input.Position - dragStart
		gui.Position = UDim2.new(
			startPos.X.Scale,
			startPos.X.Offset + delta.X,
			startPos.Y.Scale,
			startPos.Y.Offset + delta.Y
		)
	end

	-- Start dragging
	dragHandle.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position

			-- Stop dragging
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)

	-- Track mouse movement over the handle
	dragHandle.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement then
			dragInput = input
		end
	end)

	-- Move the frame
	game:GetService("UserInputService").InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			update(input)
		end
	end)

end
coroutine.wrap(YCBRB_fake_script)()
local function NSXRX_fake_script() -- Frame.fovenable 
	local script = Instance.new('LocalScript', Frame)

	local Players = game:GetService("Players")
	local UIS = game:GetService("UserInputService")
	local RunService = game:GetService("RunService")
	local GuiService = game:GetService("GuiService")
	local camera = workspace.CurrentCamera
	local localPlayer = Players.LocalPlayer

	local gui = script.Parent
	local screenGui = gui.Parent

	-- ===== UI REFERENCES =====
	local legitlock = gui:WaitForChild("Frame"):WaitForChild("Frame"):WaitForChild("aimbotgui"):WaitForChild("legitlock")
	local showFOVToggle = legitlock:WaitForChild("showfovtoggle")
	local sliderbar = legitlock:WaitForChild("sliderbar")
	local sliderBarKnob = sliderbar:WaitForChild("slider")

	local lockAimToggle = legitlock:WaitForChild("lockaimtoggle")
	local smoothnessToggle = legitlock:WaitForChild("smoothnesstoggle")
	local keybindtoggle = legitlock:WaitForChild("keybindtoggle")
	local aimparttoggle = legitlock:WaitForChild("aimparttoggle")

	local sliderbarfov = legitlock:WaitForChild("sliderbarfov")
	local sliderFOVKnob = sliderbarfov:WaitForChild("slider")
	-- ===== VARIABLES =====
	local draggingFOV = false
	local draggingSmooth = false

	-- FOV Size
	local minFOV = 50
	local maxFOV = 400
	local currentFOVSize = 200

	-- Smoothness
	local minSmooth = 0.01
	local maxSmooth = 0.30
	local currentSmooth = 0.15

	-- Key / Mouse hold tracking
	local holdKey = false

	-- ===== CREATE FOV CIRCLE =====
	local fovCircle = Instance.new("Frame")
	fovCircle.AnchorPoint = Vector2.new(0.5, 0.5)
	fovCircle.Size = UDim2.fromOffset(currentFOVSize, currentFOVSize)
	fovCircle.BackgroundTransparency = 1
	fovCircle.Visible = false
	fovCircle.Parent = screenGui

	local corner = Instance.new("UICorner")
	corner.CornerRadius = UDim.new(1, 0)
	corner.Parent = fovCircle

	local stroke = Instance.new("UIStroke")
	stroke.Thickness = 1
	stroke.Color = Color3.fromRGB(255, 255, 255)
	stroke.Parent = fovCircle

	-- ===== FOV SIZE SLIDER =====
	sliderBarKnob.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			draggingFOV = true
		end
	end)

	UIS.InputEnded:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			draggingFOV = false
		end
	end)

	UIS.InputChanged:Connect(function(input)
		if draggingFOV and input.UserInputType == Enum.UserInputType.MouseMovement then
			local mouseX = input.Position.X
			local barX = sliderbar.AbsolutePosition.X
			local barW = sliderbar.AbsoluteSize.X

			local alpha = math.clamp((mouseX - barX) / barW, 0, 1)
			sliderBarKnob.Position = UDim2.new(alpha, 0, 0.5, 0)

			currentFOVSize = minFOV + (maxFOV - minFOV) * alpha
			fovCircle.Size = UDim2.fromOffset(currentFOVSize, currentFOVSize)
		end
	end)

	-- ===== SMOOTHNESS SLIDER =====
	sliderFOVKnob.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			draggingSmooth = true
		end
	end)

	UIS.InputEnded:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			draggingSmooth = false
		end
	end)

	UIS.InputChanged:Connect(function(input)
		if draggingSmooth and input.UserInputType == Enum.UserInputType.MouseMovement then
			local mouseX = input.Position.X
			local barX = sliderbarfov.AbsolutePosition.X
			local barW = sliderbarfov.AbsoluteSize.X

			local alpha = math.clamp((mouseX - barX) / barW, 0, 1)
			sliderFOVKnob.Position = UDim2.new(alpha, 0, 0.5, 0)

			currentSmooth = minSmooth + (maxSmooth - minSmooth) * (1 - alpha)
		end
	end)

	-- ===== KEY & MOUSE DETECTION =====
	UIS.InputBegan:Connect(function(input, processed)
		if processed then return end

		local bindText = keybindtoggle.Text:lower()

		if bindText == "n/a" or bindText == "setting" then
			holdKey = false
			return
		end

		-- Mouse buttons
		if bindText == "mousebutton1" and input.UserInputType == Enum.UserInputType.MouseButton1 then
			holdKey = true
		elseif bindText == "mousebutton2" and input.UserInputType == Enum.UserInputType.MouseButton2 then
			holdKey = true
		elseif bindText == "mousebutton3" and input.UserInputType == Enum.UserInputType.MouseButton3 then
			holdKey = true

			-- Keyboard
		elseif input.UserInputType == Enum.UserInputType.Keyboard then
			if input.KeyCode.Name:lower() == bindText then
				holdKey = true
			end
		end
	end)

	UIS.InputEnded:Connect(function(input)
		local bindText = keybindtoggle.Text:lower()

		if bindText == "n/a" or bindText == "setting" then
			holdKey = false
			return
		end

		if bindText == "mousebutton1" and input.UserInputType == Enum.UserInputType.MouseButton1 then
			holdKey = false
		elseif bindText == "mousebutton2" and input.UserInputType == Enum.UserInputType.MouseButton2 then
			holdKey = false
		elseif bindText == "mousebutton3" and input.UserInputType == Enum.UserInputType.MouseButton3 then
			holdKey = false

		elseif input.UserInputType == Enum.UserInputType.Keyboard then
			if input.KeyCode.Name:lower() == bindText then
				holdKey = false
			end
		end
	end)

	-- ===== GET CLOSEST TARGET =====
	local function getClosestTarget()
		local mp = UIS:GetMouseLocation()
		local closest = nil
		local closestDist = math.huge

		for _, plr in ipairs(Players:GetPlayers()) do
			if plr ~= localPlayer and plr.Character then
				local rootPart = plr.Character:FindFirstChild(
					aimparttoggle.Text:lower() == "head" and "Head" or "HumanoidRootPart"
				)

				if rootPart then
					local screenPos, onScreen = camera:WorldToViewportPoint(rootPart.Position)
					if onScreen then
						local dx = screenPos.X - mp.X
						local dy = screenPos.Y - mp.Y
						local dist = math.sqrt(dx*dx + dy*dy)

						if dist <= (currentFOVSize / 2) and dist < closestDist then
							closestDist = dist
							closest = rootPart
						end
					end
				end
			end
		end

		return closest
	end

	-- ===== MAIN CAMERA LOOP =====
	RunService.RenderStepped:Connect(function()
		local mp = UIS:GetMouseLocation()
		local inset = GuiService:GetGuiInset()

		-- FOV circle visuals
		if showFOVToggle.Text:lower() == "on" then
			fovCircle.Visible = true
			fovCircle.Position = UDim2.new(0, mp.X, 0, mp.Y - inset.Y)
		else
			fovCircle.Visible = false
		end

		-- AIM LOCK
		if lockAimToggle.Text:lower() == "on" and holdKey then
			local target = getClosestTarget()
			if target then
				local camPos = camera.CFrame.Position
				local desiredCFrame = CFrame.new(camPos, target.Position)

				if smoothnessToggle.Text:lower() == "off" then
					camera.CFrame = desiredCFrame
				else
					camera.CFrame = camera.CFrame:Lerp(desiredCFrame, currentSmooth)
				end
			end
		end
	end)

end
coroutine.wrap(NSXRX_fake_script)()
local function JWSIB_fake_script() -- pwrware.ESPController 
	local script = Instance.new('LocalScript', pwrware)

	-- SERVICES
	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")
	local camera = workspace.CurrentCamera

	local localPlayer = Players.LocalPlayer
	local rootGui = localPlayer:WaitForChild("PlayerGui"):WaitForChild("pwrware")

	-- UI TOGGLES
	local settingsGui = rootGui:WaitForChild("Frame"):WaitForChild("Frame"):WaitForChild("Frame"):WaitForChild("visualsgui"):WaitForChild("esptab")
	local espToggle = settingsGui:WaitForChild("esptoggle")
	local nameToggle = settingsGui:WaitForChild("nametoggle")
	local healthToggle = settingsGui:WaitForChild("healthtoggle")
	local chamsToggle = settingsGui:WaitForChild("chamstoggle")

	-- STORAGE
	local espData = {}

	-- CREATE ESP ELEMENTS
	local function createESP(player)
		local data = {}

		-- Chams
		local highlight = Instance.new("Highlight")
		highlight.Adornee = nil
		highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
		highlight.Enabled = false
		highlight.Parent = rootGui
		data.highlight = highlight

		-- Name Billboard
		local nameGui = Instance.new("BillboardGui")
		nameGui.Adornee = nil
		nameGui.AlwaysOnTop = true
		nameGui.Parent = rootGui

		local nameLabel = Instance.new("TextLabel")
		nameLabel.BackgroundTransparency = 1
		nameLabel.TextColor3 = Color3.new(1,1,1)
		nameLabel.TextStrokeColor3 = Color3.new(0,0,0)
		nameLabel.TextStrokeTransparency = 0.5
		nameLabel.TextScaled = true
		nameLabel.Font = Enum.Font.SourceSansBold
		nameLabel.Size = UDim2.new(1,0,1,0)
		nameLabel.Parent = nameGui
		data.nameGUI = nameGui
		data.nameLabel = nameLabel

		-- Health Billboard (longer)
		local healthGui = Instance.new("BillboardGui")
		healthGui.Adornee = nil
		healthGui.AlwaysOnTop = true
		healthGui.Parent = rootGui

		local healthBG = Instance.new("Frame", healthGui)
		healthBG.BackgroundColor3 = Color3.new(0,0,0)
		healthBG.BorderSizePixel = 0
		healthBG.Size = UDim2.new(1,0,1,0)

		local healthBar = Instance.new("Frame", healthBG)
		healthBar.BackgroundColor3 = Color3.new(0,1,0)
		healthBar.Size = UDim2.new(1,0,1,0)
		data.healthGUI = healthGui
		data.healthBar = healthBar

		espData[player] = data
	end

	local function removeESP(player)
		local data = espData[player]
		if data then
			if data.highlight then data.highlight:Destroy() end
			if data.nameGUI then data.nameGUI:Destroy() end
			if data.healthGUI then data.healthGUI:Destroy() end
			espData[player] = nil
		end
	end

	-- INIT PLAYERS
	for _,plr in ipairs(Players:GetPlayers()) do
		if plr ~= localPlayer then
			createESP(plr)
		end
	end
	Players.PlayerAdded:Connect(function(plr)
		if plr ~= localPlayer then createESP(plr) end
	end)
	Players.PlayerRemoving:Connect(removeESP)

	-- UPDATE LOOP
	RunService.RenderStepped:Connect(function()
		local showESP    = espToggle.Text:lower() == "on"
		local showName   = nameToggle.Text:lower() == "on"
		local showHealth = healthToggle.Text:lower() == "on"
		local showChams  = chamsToggle.Text:lower() == "on"

		for player, data in pairs(espData) do
			local char = player.Character
			if not showESP or not char or not char:FindFirstChild("HumanoidRootPart") then
				-- Instead of clearing Adornee, just disable GUI elements
				data.highlight.Enabled = false
				data.nameGUI.Enabled = false
				data.healthGUI.Enabled = false
			else
				local hrp = char.HumanoidRootPart
				local humanoid = char:FindFirstChildOfClass("Humanoid")
				local dist = (camera.CFrame.Position - hrp.Position).Magnitude
				-- Cap distance scaling at 200 studs
				local scale = math.clamp(dist / 30, 1, 6)

				-- CHAMS (white)
				if showChams then
					data.highlight.Adornee = char
					data.highlight.Enabled = true
					data.highlight.FillColor = Color3.new(1,1,1)
					data.highlight.FillTransparency = 0.6
					data.highlight.OutlineColor = Color3.new(1,1,1)
					data.highlight.OutlineTransparency = 0
				else
					data.highlight.Enabled = false
				end

				-- NAME ESP
				if showName then
					data.nameGUI.Adornee = hrp
					-- place below player
					data.nameGUI.StudsOffsetWorldSpace = Vector3.new(0, -humanoid.HipHeight - 1.5, 0)
					data.nameLabel.Text = player.Name
					-- scale bigger with distance
					data.nameGUI.Size = UDim2.fromScale(2 * scale, 1 * scale)
					data.nameGUI.Enabled = true
				else
					data.nameGUI.Enabled = false
				end

				-- HEALTH ESP
				if showHealth and humanoid then
					data.healthGUI.Adornee = hrp
					-- offset beside torso, move health ESP lower
					local camRight = camera.CFrame.RightVector
					data.healthGUI.StudsOffsetWorldSpace = Vector3.new(camRight.X * 2, humanoid.HipHeight - 1.5, camRight.Z * 2)
					-- make health bar tall (head to feet relative)
					data.healthGUI.Size = UDim2.fromScale(0.15 * scale, (humanoid.HipHeight + 1) * scale)
					local pct = math.clamp(humanoid.Health / humanoid.MaxHealth, 0, 1)
					data.healthBar.Size = UDim2.new(pct, 0, 1, 0)
					-- color change
					if pct > 0.5 then
						data.healthBar.BackgroundColor3 = Color3.fromRGB(0,255,0)
					elseif pct > 0.2 then
						data.healthBar.BackgroundColor3 = Color3.fromRGB(255,255,0)
					else
						data.healthBar.BackgroundColor3 = Color3.fromRGB(255,0,0)
					end
					data.healthGUI.Enabled = true
				else
					data.healthGUI.Enabled = false
				end
			end
		end
	end)

end
coroutine.wrap(JWSIB_fake_script)()
local function RZAHB_fake_script() -- pwrware.LocalScript 
	local script = Instance.new('LocalScript', pwrware)

	local screenGui = script.Parent

	screenGui.ResetOnSpawn = false

	task.spawn(function()
		while true do
			if screenGui.DisplayOrder < 999999 then
				screenGui.DisplayOrder = 999999
			end
			task.wait(1)
		end
	end)

end
coroutine.wrap(RZAHB_fake_script)()

